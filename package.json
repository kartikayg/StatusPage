{
  "name": "components-service",
  "version": "1.0.0",
  "description": "components-service",
  "main": "src/index.js",
  "engines": {
    "node": ">=4.8.0",
    "npm": ">=2.15.11"
  },
  "bin": {
    "gulp": "./node_modules/.bin/gulp",
    "eslint": "./node_modules/.bin/eslint",
    "mocha": "./node_modules/.bin/mocha",
    "istanbul": "./node_modules/.bin/istanbul"
  },
  "scripts": {
    "dev": "gulp dev",
    "build": "gulp",
    "lint": "eslint 'src/**/*.js'",
    "flow": "flow",
    "test": "mocha --reporter spec --compilers js:babel-core/register 'src/**/*.spec.js'",
    "test:coverage": "istanbul cover _mocha -- -u exports --reporter spec --colors --compilers js:babel-core/register 'src/**/*.spec.js'"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "es6",
    "REST",
    "API"
  ],
  "author": "Kartikay Gupta <kartikayg@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "debug": "^3.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "http-status": "^1.0.1",
    "joi": "^10.6.0",
    "mongodb": "^2.2.31",
    "shortid": "^2.2.8",
    "winston": "^2.3.1",
    "winston-mongodb": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.0.5",
    "del": "^3.0.0",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "flow-bin": "^0.53.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-eslint": "^4.0.0",
    "gulp-file-cache": "0.0.1",
    "gulp-nodemon": "^2.2.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.5.3",
    "run-sequence": "^2.1.0",
    "sinon": "^3.3.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "flow"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
