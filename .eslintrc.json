{
  "parser": "babel-eslint",
  "plugins": [
    "flowtype"
  ],
  "rules": {
    "comma-dangle": ["error", "never"],
    "padded-blocks": 0,
    "object-curly-spacing": 0,
    "arrow-body-style": ["error", "always"],
    "no-underscore-dangle": 0,
    "arrow-parens": 0,
    "brace-style": ["error", "stroustrup"]
  },
  "extends": [
    "eslint:recommended",
    "airbnb-base"
  ]
}